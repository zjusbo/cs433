<FindBugsFilter>

 	<Match>
        <Class name="org.xsocket.connection.multiplexed.MultiplexedProtocolAdapter"/>
        <!-- reason: Read of unwritten field server && Field only ever set to null (which will be injected) -->
        <Bug code="NP, UwF"/>
    </Match>
    <Match>
        <Class name="org.xsocket.connection.multiplexed.MultiplexedUtils"/>
        <!-- reason: invokes java.lang.reflect.Field.setAccessible(boolean), which should be invoked from within a doPrivileged block -->
        <Bug code="DP"/>
    </Match>
</FindBugsFilter>
